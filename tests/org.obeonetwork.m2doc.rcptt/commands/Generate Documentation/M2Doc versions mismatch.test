get-view "Project Explorer" | get-tree | select "test/src/template/template.docx" | get-menu -path "Edit Template properties" | click
with [get-window "Template properties"] {
    get-button "Next >" | click
    get-editbox -after [get-label "M2Doc version:"] | set-text "-1"
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | select "test/generation/template.genconf"
exec-with-options {
    get-view "Project Explorer" | get-tree | get-menu -path "Generate Documentation" | click
}
get-window "M2Doc generation" 
    | get-label "M2Doc version mismatch: template version is -1 and current M2Doc version is 5.0.0. Do you want to continue ?" 
    | get-property caption | equals "M2Doc version mismatch: template version is -1 and current M2Doc version is 5.0.0. Do you want to continue ?" 
    | verify-true
get-window "M2Doc generation" | get-button OK | click
get-window "M2Doc generation" 
    | get-label "The document 'platform:/resource/test/generation/template-generated.docx' is generated.  The template file contains validation errors, please read 'platform:/resource/test/generation/template-validation.docx'." 
    | get-property caption | equals "The document 'platform:/resource/test/generation/template-generated.docx' is generated. \n\n"
    + " The template file contains validation errors, please read 'platform:/resource/test/generation/template-validation.docx'." 
    | verify-true
get-window "M2Doc generation" | get-button OK | click
